{
  "name": "@fronnt/components-vue",
  "version": "1.0.0",
  "type": "module",
  "types": "./dist/lib.d.ts",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "dev": "histoire dev",
    "build": "vue-tsc --noEmit && vite build",
    "build:histoire": "histoire build",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore --fix src",
    "format": "prettier --write src/**/*.{vue,ts,css}",
    "histoire:deploy": "rm -rf .histoire && histoire build && tar -cvf ./deploy.tar ./Dockerfile .histoire/* ./captain-definition && caprover deploy -t ./deploy.tar"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "peerDependencies": {
    "@fronnt/icons": "workspace:*",
    "@fronnt/styles": "workspace:*"
  },
  "devDependencies": {
    "@fontsource/barlow": "^5.0.7",
    "@histoire/plugin-vue": "^0.16.1",
    "@types/node": "^18.17.1",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.16.1",
    "histoire": "^0.16.2",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vite-plugin-dts": "^3.4.0",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.8"
  },
  "private": true
}
