{
  "name": "@fronntui/vue",
  "version": "1.0.0-alpha.0",
  "description": "fronnt UI Vue components",
  "author": "inniti <hey@inniti.de> (https://www.inniti.de)",
  "type": "module",
  "types": "./dist/lib.d.ts",
  "files": [
    "dist"
  ],
  "main": "./dist/lib.cjs",
  "module": "./dist/lib.js",
  "exports": {
    ".": {
      "import": "./dist/lib.js",
      "require": "./dist/lib.cjs"
    }
  },
  "scripts": {
    "dev": "histoire dev",
    "generate:icons": "node ./build/generate-icons.js",
    "generate:lib-entry": "node ./build/generate-lib-entry.js",
    "build": "vue-tsc --noEmit && vite build",
    "build:histoire": "histoire build",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore --fix src",
    "format": "prettier --write src/**/*.{vue,ts,css}",
    "histoire:deploy": "rm -rf .histoire && histoire build && tar -cvf ./deploy.tar ./Dockerfile .histoire/* ./captain-definition && caprover deploy -t ./deploy.tar"
  },
  "peerDependencies": {
    "@fronntui/icons": "workspace:*",
    "@fronntui/styles": "workspace:*",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@fontsource/barlow": "^5.0.8",
    "@histoire/plugin-vue": "^0.17.6",
    "@types/node": "^18.19.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.19.2",
    "histoire": "^0.17.6",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.6.4",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.25"
  }
}
