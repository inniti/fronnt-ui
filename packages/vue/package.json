{
  "name": "@fronnt/components-vue",
  "version": "1.0.0",
  "type": "module",
  "types": "./dist/lib.d.ts",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
