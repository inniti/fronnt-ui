/*! @fronntui/styles NNIcon */

.nn-icon {
  display: inline-block;
  width: 1.5em;
  height: 1.5em;
  box-sizing: border-box;
}

.nn-raw-icon {
  color: inherit;
  display: inline-block;
  stroke-width: var(--nn-icon-stroke-width);
}

.nn-icon .nn-raw-icon {
  display: block;
  width: 100%;
  height: 100%;
}
