{
  "name": "@fronnt/styles",
  "version": "1.0.0",
  "description": "CSS Stylesheets for @fronnt components",
  "main": "dist/index.css",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.css",
    "./variables": "./dist/variables.css",
    "./typography": "./dist/typography.css",
    "./components/Button": "./dist/components/Button.css",
    "./components/ActionButton": "./dist/components/ActionButton.css",
    "./components/Tag": "./dist/components/Tag.css",
    "./components/Badge": "./dist/components/Badge.css",
    "./components/Icon": "./dist/components/Icon.css",
    "./components/Input": "./dist/components/Input.css",
    "./components/LoadingIndicator": "./dist/components/LoadingIndicator.css",
    "./components/Toggle": "./dist/components/Toggle.css",
    "./components/Checkbox": "./dist/components/Checkbox.css",
    "./components/StepInput": "./dist/components/StepInput.css",
    "./components/Tabs": "./dist/components/Tabs.css",
    "./components/Dropdown": "./dist/components/Dropdown.css",
    "./components/Breadcrumbs": "./dist/components/Breadcrumbs.css",
    "./components/Alert": "./dist/components/Alert.css",
    "./components/Link": "./dist/components/Link.css",
    "./components/LabeledValue": "./dist/components/LabeledValue.css",
    "./components/Slider": "./dist/components/Slider.css",
    "./components/Textarea": "./dist/components/Textarea.css",
    "./components/Card": "./dist/components/Card.css",
    "./components/Paging": "./dist/components/Paging.css",
    "./components/Autocomplete": "./dist/components/Autocomplete.css",
