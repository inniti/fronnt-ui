  "keywords": [
    "fronnt",
    "inniti"
  ],
  "author": "inniti",
  "devDependencies": {
    "cssnano": "~6.0.1",
    "postcss": "~8.4.27",
    "postcss-cli": "~10.1.0",
    "postcss-import": "~15.1.0",
    "postcss-preset-env": "~9.1.0",
    "rimraf": "~5.0.1"
  },
  "private": true
}
