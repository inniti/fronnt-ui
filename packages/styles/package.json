    "./components/Toast": "./dist/components/Toast.css",
    "./components/Collapsible": "./dist/components/Collapsible.css",
    "./components/Range": "./dist/components/Range.css",
    "./components/Menu": "./dist/components/Menu.css",
    "./components/Calendar": "./dist/components/Calendar.css",
    "./components/Popup": "./dist/components/Popup.css",
    "./components/DatePicker": "./dist/components/DatePicker.css",
    "./components/DateRangePicker": "./dist/components/DateRangePicker.css",
    "./transitions/Expand": "./dist/transitions/Expand.css"
  },
  "scripts": {
    "build": "rimraf dist && postcss ./src/** --base src --ext css --verbose --dir dist",
    "dev": "rimraf dist && postcss ./src/** --base src --ext css --verbose --dir dist -w"
